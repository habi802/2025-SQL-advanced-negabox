<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "https://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace = "kr.co.negaboxdummy.seat.SeatMapper">
    <insert id="save">
        INSERT INTO seat
        SET screen_id = #{screenId},
            row_label = #{rowLabel},
            col_no = #{colNo},
            type = #{type}
    </insert>
</mapper>